<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Limar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul1</item>
        <item name="colorPrimaryVariant">@color/azul2</item>
        <item name="colorOnPrimary">@color/azul1</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/azul1</item>
        <item name="colorSecondaryVariant">@color/azul2</item>
        <item name="colorOnSecondary">@color/azul1</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Toobar" parent="Theme.AppCompat.NoActionBar">

    </style>

    <style name="Theme.Limar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Limar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Limar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>